{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": ["eslint:recommended"],
    "globals": [
        "$",
        "$$",
        "jQuery",
        "Promise"
    ],
    "rules": {
        "no-with": "error",
        "no-console": "warn",
        "no-array-constructor": "error",
        "no-debugger": "error",
        "no-sequences": "error",
        "no-dupe-args": "error",
        "no-inner-declarations": "error",
        "no-unreachable": "error",
        "no-octal": "error",
        "no-dupe-keys": "error",
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": "warn"
    },
    "ignorePattern": [
        "/fekit_modules/*",
        "/node_modules/*",
        "/bower_components/*",
        "/dev/*",
        "/prd/*"
    ]
}